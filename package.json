    "@auth0/auth0-spa-js": "^1.8.1",
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "history": "^4.10.1",
    "mongodb": "^3.5.7",
